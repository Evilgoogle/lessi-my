{% if item.task == 'fehlerfrei_bedienfehler' %}
<div class="item panel panel-default">
    <div class="panel-heading head">
        <div class="row">
            <div class="col-xs-12" style="font-size: 17px;">Behoben (Fehlerfrei/Bedienfehler)</div>
        </div>
    </div>
    <div class="panel-body">
        <div class="body">
            {% if data is defined %}
                {{ data.message }}
            {% else %}
                {{ item.message }}
            {% endif %}
        </div>
    </div>
</div>
